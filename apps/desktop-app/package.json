{
  "name": "desktop-app",
  "version": "0.0.2",
  "dependencies": {
    "better-sqlite3-multiple-ciphers": "12.6.2"
  },
  "devDependencies": {
    "electron": "39.6.1",
    "electron-builder": "26.8.1"
  },
  "build": {
    "extraMetadata": {
      "name": "myapp"
    },
    "appId": "com.example.desktopapp",
    "win": {
      "signtoolOptions": {
        "sign": "sign.js",
        "signingHashAlgorithms": [
          "sha256"
        ]
      },
      "signExts": [
        ".node"
      ]
    }
  }
}
